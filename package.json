		"builtin-modules": "3.3.0",
		"dotenv": "^16.4.5",
		"dedent": "^1.5.1",
		"esbuild": "^0.23.1",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4",
