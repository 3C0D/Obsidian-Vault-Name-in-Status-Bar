{
	"name": "vault-name-status-bar",
	"version": "1.1.1",
	"description": "Vault name in status bar. can be reduced to ::",
	"main": "main.js",
	"scripts": {
		"dev": "SET REAL=0 && tsx scripts/esbuild.config.mts",
		"start": "SET REAL=0 && node scripts/start.mjs",
